) {\n\t\tif (this.dirty) {\n\t\t\tthis.valiate()\n\t\t}\n\t}\n\n\tpublic async save() {\n\t\tthis.dirty = true\n\t\tif (!this.valiate()) {\n\t\t\treturn false\n\t\t}\n\t\tthis.saving = true\n\t\tif (this.mode === 'create') {\n\t\t\tthis.bill = await createBill(this.bill)\n\t\t} else {\n\t\t\tthis.bill = await editBill(this.bill.id, this.bill)\n\t\t}\n\t\tthis.$router.push('/bills')\n\t}\n\n\tpublic async remove() {\n\t\tthis.deleting = true\n\t\tawait deleteBill(this.bill.id)\n\t\tthis.$router.push('/bills')\n\t}\n\n}\n"],"sourceRoot":""}]}